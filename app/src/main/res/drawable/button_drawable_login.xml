<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_focused="true"  >
        <shape android:shape="rectangle" >
            <corners android:bottomLeftRadius="0dp"  android:bottomRightRadius="0dp"  android:radius="25dp"/>
            <stroke android:width="5dip" android:color="@color/ms_white_tarns" />
            <gradient android:angle="-90" android:startColor="@color/ms_white_tarns" android:endColor="@color/ms_white_tarns" />
        </shape>
    </item>


    <item  android:state_pressed="true" >
        <shape  android:shape="rectangle"  >

            <stroke android:width="5dip" android:color="@color/ms_white_tarns" />
            <gradient android:angle="-90" android:startColor="@color/ms_white_tarns" android:endColor="@color/ms_white_tarns" />
        </shape>
    </item>
    <item >
        <shape  android:shape="rectangle"  >
            <corners android:bottomLeftRadius="0dp"  android:bottomRightRadius="0dp"  android:radius="25dp"/>
            <stroke android:width="5dip" android:color="@color/ms_white" />
            <gradient android:angle="-90" android:startColor="@color/ms_white" android:endColor="@color/ms_white" />
        </shape>
    </item>



</selector>